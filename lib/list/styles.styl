.List
	display none

@media screen and (min-width: 768px)
	.List
		display inline-block
		text-align left
		width 40%
		vertical-align top
		text-decoration none

	.List ul
		list-style-type none
		margin 0
		padding 0

	.List-group
		padding 0
		margin 0 0 2rem 3rem 
		text-align left

	.List-groupIcon:before
		font-family 'Flaticon'
		content "\e000"
		font-size 2rem
		float left